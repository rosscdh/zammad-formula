{%- from "zammad/map.jinja" import config with context %}
IMAGE_REPO={{ config.image }}
POSTGRESQL_HOST={{ config.env.POSTGRESQL_HOST | default('zammad-postgresql') }}
POSTGRES_USER={{ config.env.POSTGRES_USER | default('zammad') }}
POSTGRES_PASSWORD={{ config.env.POSTGRES_PASSWORD | default('zammad') }}
POSTGRESQL_DB={{ config.env.POSTGRES_DB | default('zammad_production') }}
RESTART={{ config.restart | default('always') }}
